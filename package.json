{
  "name": "onetimeurl",
  "version": "0.1.0",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "yarn build:server && yarn build:client",
    "build:server": "bsb -make-world",
    "build:client": "parcel build src/client.bs.js -d public/ -o bundle.js",
    "watch": "bsb -make-world -w",
    "start": "yarn build && node ./src/server.bs.js"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "bs-platform": "^2.2.2",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "bs-apollo-server-express": "^0.3.1",
    "bs-express": "^0.4.1",
    "bs-graphql": "^0.3.1",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "reason-react": "^0.3.4",
    "serve-static": "^1.13.2",
    "serverless": "^1.26.1",
    "serverless-offline": "^3.18.0"
  }
}
