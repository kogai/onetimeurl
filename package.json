{
  "name": "onetimeurl",
  "version": "0.1.0",
  "scripts": {
    "clean": "bsb -clean-world",
    "build": "yarn build:server && yarn build:client",
    "build:server": "bsb -make-world",
    "build:client": "parcel build src/client.bs.js -d public/ -o bundle.js",
    "build:container": "docker-compose build",
    "watch": "bsb -make-world -w",
    "psql": "docker-compose exec db psql",
    "psql:login": "yarn psql onetimeurl -U postgres",
    "start": "yarn build && docker-compose up"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "bs-platform": "^2.2.3",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "@types/pg": "^7.4.5",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "bs-apollo-server-express": "^0.3.1",
    "bs-express": "^0.4.1",
    "bs-graphql": "^0.3.1",
    "bs-graphql-tools": "^0.3.1",
    "bs-json": "^1.0.1",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "graphql-tools": "^2.23.1",
    "pg": "^7.4.1",
    "reason-react": "^0.3.4",
    "serve-static": "^1.13.2"
  }
}
